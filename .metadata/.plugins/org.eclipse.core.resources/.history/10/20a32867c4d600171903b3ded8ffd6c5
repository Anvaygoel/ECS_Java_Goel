
public class YourSchedule 
{
	public static void main(String[] args)
	{
		String per1, per2, per3, per4, tech2, tech3, tech4;
		String tech1;
		
		
		per1 = "Biology";
		per2 = "ECS";
		per3 = "English";
		per4 = "ISPE";
		tech1 = "Lawless";
		tech2 = "Mauro";
		tech3 = "Thow";
		tech4 = "Thorton";
		
		System.out.println("|Per 1 | " + per1 + " | " + tech1 + "|" );
		System.out.println("|Per 1 | " + per2 + "     | " + tech2 + "  |" );
		System.out.println("|Per 1 | " + per3 + " | " + tech3 + "   |" );
		System.out.println("|Per 1 | " + per4 + "    | " + tech4 + "|" );
		System.out.println("---------------------------");
	}

}
