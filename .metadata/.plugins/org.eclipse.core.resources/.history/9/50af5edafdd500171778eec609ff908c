
public class UnsingVaribles
{
	public static void main(String[] args)
	{
		int  Room_Number;
		double Subject, Subject2, computer_science = 0;
		
		
		Room_Number=113;
		Subject=2.71828;
		
		Subject2=computer_science;
		
		System.out.println("This is room #" + Room_Number );
		System.out.println(" e is close to " + Subject );
		System.out.println("I'm learing a bit about " + Subject2 );
		
				
		
	}

}
