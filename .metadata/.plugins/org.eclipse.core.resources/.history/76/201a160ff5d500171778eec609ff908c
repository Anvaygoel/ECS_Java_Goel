
public class YourInitials
{
	public static void main(String[] args)
	{
		System.out.println("The initials for Anvay Goel");
		System.out.println("           A");
		System.out.println("          A A");
		System.out.println("        A     A");
		System.out.println("       A       A");
		System.out.println("       A A A A A");
		System.out.println("       A       A");
		System.out.println("       A       A");
		
	}

}
